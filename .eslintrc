{
    "env": {
        "browser": true,
        "commonjs": true,
        "amd": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "plugin:requirejs/recommended",
        "prettier"
    ], 
    "parserOptions": {
        "ecmaFeatures": {
            "experimentalObjectRestSpread": true,
            "jsx": true
        },
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "requirejs",
        "prettier"
    ],
    "rules": {
        "prettier/prettier" : ["error", {
            "printWidth": 120,
            "tabWidth": 4,
            "useTabs": false,
            "singleQuote": true,
            "bracketSpacing": true
        }],
        "accessor-pairs": [1],
        "block-scoped-var": 1,
        "camelcase": [2,{"properties":"always"}],
        "consistent-return": 1,
        "constructor-super": 2,
        "curly": [2,"all"],
        "default-case": 2,
        "dot-notation": [2,{"allowKeywords":true}],
        "eqeqeq": [2,"always"],
        "func-style": [2,"expression"],
        "guard-for-in": 1,
        "handle-callback-err": 1,
        "linebreak-style": [ "error", "unix" ],
        "linebreak-style": [2,"unix"],
        "max-nested-callbacks": [2,4],
        "new-cap": [2,{"newIsCap":true,"capIsNew":true}],
        "newline-after-var": [2,"always"],
        "no-alert": 1,
        "no-array-constructor": 2,
        "no-caller": 2,
        "no-catch-shadow": 2,
        "no-class-assign": 2,
        "no-cond-assign": 2,
        "no-const-assign": 2,
        "no-constant-condition": 1,
        "no-console": 0,
        "no-case-declarations": 0,
        "no-debugger": 1,
        "no-delete-var": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-else-return": 1,
        "no-empty": 1,
        "no-empty-character-class": 2,
        "no-eval": 2,
        "no-ex-assign": 2,
        "no-extend-native": 2,
        "no-extra-bind": 1,
        "no-extra-boolean-cast": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-func-assign": 1,
        "no-implicit-coercion": [2,{"number":true,"string":true}],
        "no-implied-eval": 2,
        "no-inner-declarations": [2,"functions"],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-iterator": 1,
        "no-label-var": 2,
        "no-lone-blocks": 2,
        "no-lonely-if": 2,
        "no-loop-func": 2,
        "no-mixed-requires": 1,
        "no-mixed-spaces-and-tabs": 2,
        "no-multi-spaces": 2,
        "no-multi-str": 1,
        "no-multiple-empty-lines": [2,{"max":2}],
        "no-native-reassign": 1,
        "no-negated-in-lhs": 2,
        "no-new": 2,
        "no-new-func": 2,
        "no-new-object": 2,
        "no-new-require": 2,
        "no-new-wrappers": 2,
        "no-obj-calls": 2,
        "no-octal": 2,
        "no-octal-escape": 1,
        "no-param-reassign": 1,
        "no-path-concat": 2,
        "no-process-exit": 1,
        "no-proto": 1,
        "no-redeclare": 2,
        "no-regex-spaces": 1,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 1,
        "no-shadow-restricted-names": 2,
        "no-sparse-arrays": 2,
        "no-sync": 1,
        "no-throw-literal": 1,
        "no-trailing-spaces": [2,{"skipBlankLines":true}],
        "no-undef": 2,
        "no-undef-init": 2,
        "no-unexpected-multiline": 0,
        "no-unneeded-ternary": 2,
        "no-unreachable": 1,
        "no-unused-expressions": 1,
        "no-unused-vars": 2,
        "no-use-before-define": [2,"nofunc"],
        "no-useless-call": 1,
        "no-var": 1,
        "no-with": 2,
        "object-shorthand": [1,"always"],
        "operator-assignment": [2,"never"],
        "prefer-const": 2,
        "prefer-spread": 1,
        "quotes": [2,"single","avoid-escape"],
        "require-yield": 2,
        "strict": [1,"global"],
        "use-isnan": 2,
        "valid-typeof": 2,
        "vars-on-top": 1,
        "yoda": 2
    }
}
